new file mode 100644
--- /dev/null
+++ b/browser/base/content/browser-getUserMedia.js
@@ -0,0 +1,125 @@
+# -*- Mode: javascript; tab-width: 2; indent-tabs-mode: nil; c-basic-offset: 2 -*-
+# This Source Code Form is subject to the terms of the Mozilla Public
+# License, v. 2.0. If a copy of the MPL was not distributed with this
+# file, You can obtain one at http://mozilla.org/MPL/2.0/.
+
+let getUserMedia = {
+
+  init: function() {
+    Services.obs.addObserver(this, "getUserMedia:request", false);
+  },
+
+  uninit: function() {
+    Services.obs.removeObserver(this, "getUserMedia:request");
+  },
+
+  observe: function(aSubject, aTopic, aData) {
+    if (aTopic != "getUserMedia:request") {
+      return;
+    }
+    try {
+      let params = aSubject.QueryInterface(Ci.nsIMediaStreamOptions);
+      this.handleRequest(aData, params);
+    } catch(e) {
+      Cu.reportError(e);
+    }
+  },
+
+  handleRequest: function(aRequestID, aParams) {
+    navigator.mozGetUserMediaDevices(
+      function (devices) {
+        this.prompt(aRequestID, aParams.audio, aParams.video, devices);
+      }.bind(this),
+      function (error) {
+        Cu.reportError(error);
+      }
+    );
+  },
+
+  prompt: function(aRequestID, aAudio, aVideo, aDevices) {
+    let audioDevices = [];
+    let videoDevices = [];
+    for (let device of aDevices) {
+      device = device.QueryInterface(Ci.nsIMediaDevice);
+      switch (device.type) {
+        case "audio":
+          if (aAudio) {
+            audioDevices.push(device);
+          }
+          break;
+        case "video":
+          if (aVideo) {
+            videoDevices.push(device);
+          }
+          break;
+      }
+    }
+
+    let requestType;
+    if (audioDevices.length && videoDevices.length) {
+      requestType = "shareCameraAndMicrophone";
+    } else if (audioDevices.length) {
+      requestType = "shareMicrophone";
+    } else if (videoDevices.length) {
+      requestType = "shareCamera";
+    } else {
+      return;
+    }
+
+    let browser = gBrowser.selectedBrowser;
+    let host = browser.contentDocument.documentURIObject.asciiHost;
+    let message = gNavigatorBundle.getFormattedString("getUserMedia." + requestType + ".message",
+                                                      [ host ]);
+
+    let responseSent = false;
+
+    let mainAction = {
+      label: gNavigatorBundle.getString("getUserMedia." + requestType + ".label"),
+      accessKey: gNavigatorBundle.getString("getUserMedia." + requestType + ".accesskey"),
+      callback: function() {
+        Services.obs.notifyObservers(
+          null, "getUserMedia:response:allow", aRequestID
+        );
+        responseSent = true;
+      }
+    };
+
+    let secondaryActions = [];
+    let selectableDevices = videoDevices.length ? videoDevices : audioDevices;
+    if (selectableDevices.length > 1) {
+      let selectableDeviceNumber = 0;
+      for (let device of selectableDevices) {
+        secondaryActions.push({
+          label: gNavigatorBundle.getFormattedString(
+                   device.type == "audio" ?
+                     "getUserMedia.shareSpecificMicrophone.label" :
+                     "getUserMedia.shareSpecificCamera.label",
+                   [ device.name ]),
+          accessKey: ++selectableDeviceNumber,
+          callback: function() {
+            Services.obs.notifyObservers(
+              device, "getUserMedia:response:allow", aRequestID
+            );
+            responseSent = true;
+          }
+        });
+      }
+    }
+
+    let options = {
+      removeOnDismissal: true,
+      eventCallback: function(aType) {
+        if (!responseSent && aType == "removed") {
+          Services.obs.notifyObservers(
+            null, "getUserMedia:response:deny", aRequestID
+          );
+        }
+      }
+    };
+
+    let notification =
+      PopupNotifications.show(browser, "getUserMedia-shareDevices", message,
+                              "getUserMedia-notification-icon", mainAction,
+                              secondaryActions, options);
+  }
+};
